@model List<ChartNado.Models.Dev.Entities.DevEmployee>

@{
    ViewBag.Title = "Employees";
}

<h2>Employees</h2>

<table>
    <thead>
    <tr>
        <th>Guid</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Salary</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var employee in @Model)
    {
        <tr>
            <td>@employee.Id</td>
            <td>@employee.FirstName</td>
            <td>@employee.LastName</td>
            <td>@employee.Age</td>
            <td>@employee.Salary</td>
        </tr>
    }
    </tbody>
</table>